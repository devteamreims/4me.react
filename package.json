{
  "name": "4me.react",
  "description": "4ME User interface",
  "version": "0.1.0",
  "author": "Benjamin BERET <benjamin.beret@gmail.com>",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/nicksp/redux-webpack-es6-boilerplate.git"
  },
  "bugs": "https://github.com/nicksp/redux-webpack-es6-boilerplate/issues",
  "scripts": {
    "clean:build": "rm -rf build && mkdir build",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "rm -rf build",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "NODE_ENV=production webpack -p --config ./config/webpack.config.prod.js --progress",
    "build:demo": "NODE_ENV=production && rm -rf demo && mkdir demo && node ./bin/buildDemo.js",
    "build:demo_old": [
      "NODE_ENV=production",
      "rm -rf demo && mkdir demo",
      "&& npm run clean:build && npm run build:html",
      "&& webpack -p --config ./config/webpack.config.demo.js --progress --cwp-id 32",
      "&& mv build demo/cwp32",
      "&& npm run clean:build && npm run build:html",
      "&& npm run build:html && webpack -p --config ./config/webpack.config.demo.js --progress --cwp-id 33",
      "&& mv build demo/cwp33"
    ],
    "lint": "eslint config src/**/*.js src/**/*.jsx",
    "jscs": "jscs src/js/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.8.0",
    "babel-eslint": "4.1.8",
    "babel-loader": "6.2.3",
    "babel-preset-async-to-bluebird": "1.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "0.3.3",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jscs": "2.10.1",
    "node-sass": "3.7.0",
    "progress-bar-webpack-plugin": "1.6.0",
    "react-addons-perf": "0.14.7",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.2.0",
    "redux-devtools-dock-monitor": "1.1.0",
    "redux-devtools-log-monitor": "1.0.4",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "axios": "0.9.1",
    "bluebird": "3.3.4",
    "classnames": "2.2.3",
    "lodash": "4.5.0",
    "material-ui": "0.15.0-alpha.2",
    "moment": "2.12.0",
    "node-neat": "1.7.2",
    "react": "0.14.7",
    "react-addons-shallow-compare": "0.14.8",
    "react-dom": "0.14.7",
    "react-highlighter": "0.3.1",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "react-tap-event-plugin": "0.2.2",
    "redux": "3.3.1",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1",
    "reselect": "2.2.1",
    "socket.io-client": "1.4.5"
  }
}
