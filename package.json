{
  "name": "4me.react",
  "description": "4ME User interface",
  "version": "0.2.5",
  "author": "Benjamin BERET <benjamin.beret@gmail.com>",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/devteamreims/4me.react.git"
  },
  "bugs": "https://github.com/devteamreims/4ME/issues",
  "scripts": {
    "preversion": "git add Changelog.md",
    "clean:build": "rm -rf build && mkdir build",
    "prestart": "rm -rf build",
    "start": "NODE_ENV=development webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build",
    "build:ci": "NODE_ENV=production webpack -p --config ./config/webpack.config.prod.js",
    "build": "npm run build:ci -- --progress",
    "build:demo": "NODE_ENV=production && rm -rf demo && mkdir demo && node ./bin/buildDemo.js",
    "build:demo_old": [
      "NODE_ENV=production",
      "rm -rf demo && mkdir demo",
      "&& npm run clean:build && npm run build:html",
      "&& webpack -p --config ./config/webpack.config.demo.js --progress --cwp-id 32",
      "&& mv build demo/cwp32",
      "&& npm run clean:build && npm run build:html",
      "&& npm run build:html && webpack -p --config ./config/webpack.config.demo.js --progress --cwp-id 33",
      "&& mv build demo/cwp33"
    ],
    "lint": "eslint 'src/**/*.js' 'src/**/*.jsx'",
    "test": "echo \"No test specified\" && exit 0",
    "check-deps": "npm-check --production --skip-unused"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.14.0",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.5",
    "babel-preset-async-to-bluebird": "1.1.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "6.13.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "node-sass": "3.9.3",
    "postcss-loader": "0.13.0",
    "progress-bar-webpack-plugin": "1.9.0",
    "react-addons-perf": "15.3.1",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "2.1.0-beta.4"
  },
  "dependencies": {
    "axios": "0.14.0",
    "bluebird": "3.4.6",
    "bourbon": "4.2.7",
    "bourbon-neat": "1.8.0",
    "classnames": "2.2.5",
    "lodash": "4.15.0",
    "material-ui": "0.16.1",
    "moment": "2.15.0",
    "node-neat": "1.7.2",
    "npm-check": "5.2.3",
    "ramda": "0.22.1",
    "react": "15.3.1",
    "react-addons-shallow-compare": "15.3.1",
    "react-dom": "15.3.1",
    "react-highlighter": "0.3.3",
    "react-redux": "4.4.5",
    "react-router": "4.0.0-alpha.5",
    "react-tap-event-plugin": "1.0.0",
    "recompose": "0.20.2",
    "redux": "3.6.0",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.3",
    "socket.io-client": "1.4.8"
  }
}
