// FlowTypes for FMP

export type Arcid = string;
export type ElementarySector = string;
export type StamId = string;

export type Flight = {
  arcid: Arcid,
  constraint: {
    beacon: string,
    flightLevel: number,
  },
  implementingSector: ElementarySector,
  onloadSector: ElementarySector,
};

export type PreparedStam = {
  offloadSector: ElementarySector,
  flights: Array<Flight>,
  id: StamId,
  sendTime: ?Date,
  archiveTime: ?Date,
  createdAt: Date,
  updatedAt: Date,
};

export type ActiveStam = PreparedStam;

export type HistoryStam = {

};
